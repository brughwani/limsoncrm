{
  "hosting": {
    "public": "build/web",
    "headers": [
      {
        "source": "/*",
        "headers": [
          {
            "key": "x-robots-tag",
            "value": "noindex"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]


  }
}

